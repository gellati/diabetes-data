#!/usr/bin/env python
# -*- coding: utf-8 -*-

import os
import csv

from datetime import datetime, time

class HeartRate(object):
    def __init__(self): pass
        

    def readDataFile():
        file = 'HealthKit/HeartRate.csv'
        datarows = []
        with open(file, 'rb') as f:
            reader = csvreader(f)
            for row in reader:
                datarows.append(row)
        return datarows

    def parseDataFile():
        datarows = self.readDataFile()
        data = []
        for r in datarows:
            [start, finish, heartrate] = r.split(',')
            data.append
        return datarows
